<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/cabaio"
    android:layout_height="match_parent"
    tools:context=".menuterror">

    <TextView
        android:id="@+id/textviewbebidas1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="104dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="105dp"
        android:text="@string/textviewbebidas1"
        android:textColor="#121111"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textviewbebidas2"
        android:layout_width="356dp"
        android:layout_height="53dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="72dp"
        android:text="@string/textviewbebidas2"
        android:textColor="#121111"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/video1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textviewbebidas1"
        tools:ignore="TextSizeCheck" />

    <VideoView
        android:id="@+id/video1"
        android:layout_width="match_parent"
        android:layout_height="270dp"
        android:layout_marginTop="72dp"
        android:layout_marginBottom="280dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textviewbebidas2" />

</androidx.constraintlayout.widget.ConstraintLayout>